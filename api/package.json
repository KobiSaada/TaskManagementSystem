{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "tsc && node ./dist/index.js",
    "dev": "nodemon --watch 'src/**/*' --exec 'tsc && node ./dist/index.js'",
    "test": "jest",
    "build": "tsc"
  },
  "dependencies": {
    "18": "^0.0.0",
    "amazon-cognito-identity-js": "^6.3.12",
    "aws-sdk": "^2.1667.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "mongoose": "^8.5.2",
    "morgan": "^1.10.0",
    "winston": "^3.13.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mocha": "^10.0.7",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^22.1.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.2.4",
    "typescript": "^5.5.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "./test"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/?(*.)+(spec|test).[tj]s?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
